{
  "name": "openapi-mock-middleware",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn build -w",
    "build": "yarn build:ts",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "watch:node": "nodemon example/example.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch:ts\" \"yarn watch:node\"",
    "lint": "eslint --ext=ts .",
    "pretest": "yarn lint",
    "test": "jest --forceExit --collectCoverageFrom=\"src/**/*.ts\" --coverage --verbose",
    "test:watch": "jest --forceExit --verbose  --watchAll",
    "security": "yarn audit",
    "release": "standard-version",
    "commit": "git-cz"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "ajv": "^8.8.2",
    "chokidar": "^3.5.2",
    "cookie-parser": "^1.4.6",
    "debug": "~2.6.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "json-schema-faker": "^0.5.0-rcv.40",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "openapi-types": "^10.0.0",
    "path-to-regexp": "^6.2.0",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/method-override": "^0.0.32",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "commitizen": "^4.2.4",
    "concurrently": "^6.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "json-schema-faker-types": "^0.1.6",
    "nodemon": "^2.0.15",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
